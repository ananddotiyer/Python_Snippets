   "metadata": {},
   "outputs": [],
   "source": [
    "aList = [15, 6, 13, 22, 3, 52, 2]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(1,len(aList)):\n",
    "    key = aList[i]\n",
    "    j = i - 1\n",
    "    while j >= 0 and key < aList[j]:\n",
    "        aList[j+1] = aList[j]\n",
    "        j = j - 1\n",
    "    else:\n",
    "        aList[j + 1] = key"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "List after sorting: [2, 3, 6, 13, 15, 22, 52]\n"
     ]
    }
   ],
   "source": [
    "print (\"List after sorting:\", aList)"
   ]
  }
